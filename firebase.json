{
  "firebase": "documentation-viewer",
  "public": "build",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**"
  ],
  "rewrites": [
    {
      "source": "**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source" : "**",
      "headers" : [
        {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }
      ]
    }
  ]
}
